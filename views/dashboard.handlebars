    <h1>Welcome to your dashboard-o-blog!</h1>
    <div class="row" id="userBlogs">
        <div class="col-md-6">
            <h3 class="text-center text-weight-bold">My Blogs</h3>
        </div>
        <ul>
            Blogs:{{#each userBlogs}}
            <li>{{this.title}}</li>
            {{/each}}
        </ul>
        <div class="input-group-mb-3">
            <input type="text" id="title-post" class="form-control" placeholder="title of new blog">
            <input type="text" class="form-control" aria-describedby="sizing-lg" id="text-post">
            <button class="bi bi-file-word-fill" id="postBlogBtn" data-value="{{userId}}">Post Blog</button>
        </div>
    <a href="/posted">Go to the posted section to see any comments!</a>
        
    </div>